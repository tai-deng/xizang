body {
    background-image: url(../../../imgs/bg.png);
    margin: 0;
}

.details_main {
    width: 100%;
    height: 100%;
    font-size: 0.18rem;
    color: #A3C3F7;
    margin-left: 0.81rem;
}

.header {
    width: 100%;
    height: 0.62rem;
    font-size: 0.18rem;
    border-bottom: 1px solid #1C44A9;
    background-color: #000340;
}

.header .header_title {
    display: inline-block;
    width: 7.86rem;
    font-size: 0.32rem;
    color: #D1DFFF;
    line-height: 0.82rem;
    text-align: center;
    margin-left: 3.78rem;
    letter-spacing: 2px;
    background-image: url(../../../imgs/title_bar.png)
}

.header .header_content {
    display: inline-block;
    line-height: 0.62rem;
    vertical-align: top;
    margin-left: 2.5rem;
}

.header .header_content .exhibition,
.header .header_content .platform {
    display: inline-block;
    width: 0.94rem;
    line-height: 0.36rem;
    color: #ABCBFF;
    text-align: center;
    border: 1px solid #4574D6;
}

.header .header_content .platform {
    margin-left: 0.3rem;
}

.header .header_content .date {
    margin-left: 0.3rem;
    color: #72A6FC;
}

.active {
    box-shadow: 0px 0px 4px 0px rgba(53, 128, 255, 1);
}





.details_main_body {
    width: 100%;
    height: 9.6rem;
    padding-top: 0.5rem;
    overflow: hidden;
}

/* 左边 */
.details_main_left {
    width: 8.2rem;
    height: 9.4rem;
    display: inline-block;
    border: 0.01rem solid #4574D6;
    /* padding:0.14rem; */
    box-sizing: border-box;
    float: left;
}

.details_main_left_title {
    padding-bottom: 0.1rem;
    width: 100%;
}

.details_main_left_title select {
    background-color: #0B1A52
}

.details_select_left {
    border: 0.01rem solid #3E6BCD;
    width: 2.9rem;
    height: 0.36rem;
    color: #A3C3F7;
}

.details_select_right {
    border: 0.01rem solid #3E6BCD;
    float: right;
    width: 4.76rem;
    height: 0.36rem;
    padding: 0.08rem 0.11rem;
    color: #A3C3F7;
}

.details_main_left_up {
    padding: 0.08rem 0.11rem 0rem 0.11rem;

}

.details_main_left_echart {
    width: 100%;
    height: 4.2rem;
    box-sizing: border-box;
    border: 0.01rem solid #416ECF;
    background-color: #4674D6;
}

.details_main_left_building_head {
    width: 100%;
    border-bottom: 0.02rem solid #1c3889;
    padding: 0.17rem 0.22rem;
    font-size: 0.24rem;
    box-sizing: border-box;
    vertical-align: -webkit-baseline-middle;
}

.details_main_left_building_head span {
    font-size: 0.14rem;
}

.details_main_left_building_body {
    padding: 0.17rem 0.22rem;
    overflow-x: hidden;
    overflow-y: scroll;
    height: 3.18rem;
}

/*滚动条整体样式-高宽分别对应横竖滚动条的尺寸*/
.details_main_left_building_body::-webkit-scrollbar {
    width: 0.1rem;
    height: 0.1rem;
}

/*滚动条里面小方块*/
.details_main_left_building_body::-webkit-scrollbar-thumb {
    border-radius: 0.05rem;
    -webkit-box-shadow: inset 0 0 5px rgba(8, 38, 126, 0.2);
    background: rgba(8, 38, 126, 0.2);
}

/*滚动条里面轨道*/
.details_main_left_building_body::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 5px rgba(3, 22, 90, 0.2);
    border-radius: 0;
    background: rgba(3, 22, 90, 0.1);
}

.details_main_foot_right {
    margin-left: 0.5rem;
}


.details_main_left_building_body ul {
    margin: 0rem;
    list-style: none;
    padding-inline-start: 0;
}

.details_main_left_building_body li {
    display: inline-block;
    float: left;
    text-align: left;
    padding: 0.1rem 0;
    font-size: 0.18rem;
}

.details_main_left_building_body li span {
    color: #ffffff;
}

.details_display_left_li {
    width: 55%;
}

.details_display_right_li {
    width: 45%;
}

.details_display_all_li {
    width: 100%;
}

.details_main_left_building_head p {
    margin: 0;
}

.details_main_left_head_right {
    float: right;
}

.details_main_left_head_img {
    margin-left: 0.2rem;
    position: relative;
    top: 0.05rem;
}

.details_main_left_head_list i {
    display: inline-block;
    padding: 0.03rem 0.11rem;
    background-color: #4674D6;
    color: #ffffff;
    text-align: center;
    margin-left: 0.2rem;
    font-size: 0.14rem;
    font-style: normal;
}

.details_main_left_foot {
    width: 100%;
    height: 0.45rem;
    background-color: #08267E;
    color: #ffffff;
    text-align: center;
    font-size: 0.18rem;
    line-height: 0.45rem;
}

/* 右边 */
.details_main_right {
    width: 9.21rem;
    height: 9.4rem;
    margin-left: 0.2rem;
    display: inline-block;
    box-sizing: border-box;
    float: left;

}

/* 投资总额 */
.details_main_right1 {
    box-sizing: border-box;
    width: 100%;
    height: 2.34rem;
    margin-bottom: 0.1rem;
    border: 0.01rem solid #4574D6;
}

.details_main_right1>div {
    display: inline-block;
    float: left;
    padding: 0.08rem 0.2rem;
    box-sizing: border-box;
}

.right1_div_left {
    width: 40%;
    border-bottom: 0.01rem solid #2C50A7;
    box-sizing: border-box;
}

.right1_div_left>p {
    width: 100%;
    margin: 0;
    text-align: center;
    border-right: 0.01rem solid #2C50A7;
}

.right1_div_right {
    width: 60%;
    border-bottom: 0.01rem solid #2C50A7;
    box-sizing: border-box;
}

.right1_div_right>p {
    width: 100%;
    margin: 0;
    text-align: center;
}

.right1_div_right>div {
    width: 50%;
    box-sizing: border-box;
    float: left;
    padding-left: 10%;
    height: 100%;
}

.right1_div_right>div p {
    margin: 0;
    font-size: 0.14rem;
}

.right1_largerText {
    font-size: 0.28rem;
    color: #ffffff;
}

.details_main_right1 .right1_noPadding {
    padding: 0;
}

.right1_div_height {
    height: 0.58rem;
}

.right1_div_right_padding {
    border-right: 0.01rem solid #2C50A7;
    padding-top: 0.1rem;
    box-sizing: border-box;
}

.white_text_right1 {
    color: #ffffff;
}

.right1_div_left>div {
    display: inline-block;
    box-sizing: border-box;
    float: left;
}

.right1_div_left_height>div:first-child {
    width: 30%;
    border-right: 0.01rem solid #2C50A7;
    height: 100%;
    box-sizing: border-box;
    padding-top: 0.2rem;
    padding-left: 0.2rem
}

.right1_div_left_height>div:last-child {
    width: 70%;
    border-right: 0.01rem solid #2C50A7;
    height: 100%;
    font-size: 0.16rem;
    padding-left: 0.2rem;
    box-sizing: border-box;
}

.padding_top_p_right1 p {
    line-height: 0.8rem
}

.right1_div_left_height {
    height: 0.92rem;
    padding: 0;
}

.right1_div_left_height>div>p {
    margin: 0;
    padding-top: 0.15rem;
}

/* 投资清单 */
.details_main_right2 {
    box-sizing: border-box;
    width: 100%;
    margin-bottom: 0.1rem;
    border: 0.01rem solid #4574D6;
    height: 2.36rem;
}

.details_main_right2_head {
    width: 100%;
    height: 0.47rem;
    text-align: center;
    font-size: 0.2rem;
    line-height: 0.47rem;
    background-color: #0A2883;
}

.details_main_right2_table {
    width: 100%;
    height: 1.88rem;
    padding: 0.06rem 0.08rem;
    box-sizing: border-box;
    overflow-x: scroll;
    overflow-y: hidden;
    font-size: 0.14rem;
}

/*滚动条整体样式-高宽分别对应横竖滚动条的尺寸*/
.details_main_right2_table::-webkit-scrollbar,
.details_main_right4_body::-webkit-scrollbar {
    width: 0.1rem;
    height: 0.1rem;
}

/*滚动条里面小方块*/
.details_main_right2_table::-webkit-scrollbar-thumb,
.details_main_right4_body::-webkit-scrollbar-thumb {
    border-radius: 0.05rem;
    -webkit-box-shadow: inset 0 0 5px rgba(53, 90, 175, 0.8);
    background: rgba(53, 90, 175, 0.8);
}

/*滚动条里面轨道*/
.details_main_right2_table::-webkit-scrollbar-track,
.details_main_right4_body::-webkit-scrollbar-track {
    -webkit-box-shadow: inset 0 0 5px rgba(209, 224, 255, 0.8);
    border-radius: 0;
    background: rgba(209, 224, 255, 0.8);
}

.details_table td {
    text-align: center;
}

.details_first_td {
    width: 1.2rem
}

table,
table tr th,
table tr td {
    border: 1px solid #0094ff;
}

table {
    min-height: 25px;
    line-height: 25px;
    text-align: center;
    border-collapse: collapse;
}



/* 项目进度 */
.details_main_right3 {
    box-sizing: border-box;
    overflow: hidden;
    width: 100%;
    height: 2.34rem;
    margin-bottom: 0.1rem;
    border: 0.01rem solid #4574D6;
}

.details_main_right2_head p {
    margin: 0;
    padding-left: 1.6rem;
}

.details_main_right2_head_right {
    float: right;
    font-size: 0.15rem;
    margin-right: 0.2rem;
}

.details_right3_p {
    margin: 0.5rem auto;
    margin-bottom: 0.1rem;
    display: block;
    width: 7rem;
    height: 0.42rem;
    background-color: #ffffff;
    border-radius: 0.42rem;

}

.details_right3_span {
    display: block;
    width: 70%;
    height: 100%;
    background-color: #3DCAFC;
    text-align: center;
    color: #ffffff;
    border-radius: 0.42rem;
    line-height: 0.42rem
}

.details_right3_div {
    float: left;
    display: inline-block;
    width: 30%;
    height: 1rem;
    padding-left: 1rem;
    font-size: 0.16rem;
}

.details_main_right3_body>div:last-child {
    float: right;
    display: inline-block;
    width: 30%;
    height: 1rem;
    padding-right: 1rem;
    padding-left: 0.5rem;
    box-sizing: border-box;
    font-size: 0.16rem;
}

/* 项目资料详情 */
.details_main_right4 {
    box-sizing: border-box;
    width: 100%;
    height: 2.29rem;
    margin-bottom: 0.1rem;
    border: 0.01rem solid #4574D6;
}

.details_main_right4_head p {
    margin: 0;
    padding-left: 1.7rem;
}

.details_main_right4_head {
    width: 100%;
    height: 0.47rem;
    text-align: center;
    font-size: 0.2rem;
    line-height: 0.47rem;
    background-color: #0A2883;
}

.details_main_right4_body {
    width: 100%;
    height: 1.82rem;
    overflow-x: scroll;
    overflow-y: hidden;
}

.details_main_right4_body ul {
    margin: 0rem;
    list-style: none;
    padding-inline-start: 0;
}

.details_main_right4_body li {
    display: inline-block;
    float: left;
    text-align: center;
    margin-left: 0.3rem;
    padding: 0.1rem 0;
    font-size: 0.18rem;
}

.details_main_right4_body li p {
    margin: 0;
}

.return {
    width: 1.08rem;
    height: 0.36rem;
    background: rgba(11, 26, 82, 0.24);
    border: 1px solid rgba(69, 116, 214, 1);
    box-shadow: 0rem 0rem 0.04rem 0rem rgba(53, 128, 255, 1);
    font-size: 0.18rem;
    font-family: SourceHanSansCN-Regular;
    font-weight: 400;
    color: rgba(171, 203, 255, 1);
    line-height: 0.36rem;
    text-align: center;
    float: left;
    margin-top: 0.12rem;
    margin-left: 0.81rem;
    cursor: pointer;
}

/* 左右箭头 */
.left_arrow {
    background: url("../../../imgs/left_arrow.png") no-repeat;
    width: 0.34rem;
    height: 0.63rem;
    float: left;
    margin-top: 4.47rem;
    margin-left: 0.24rem;
    background-size: 100% 100%;
}

.left_arrow_active {
    background: url("../../../imgs/left_arrow_active.png") no-repeat;
    width: 0.34rem;
    height: 0.63rem;
    float: left;
    margin-top: 4.47rem;
    margin-left: 0.24rem;
    background-size: 100% 100%;
}

.right_arrow {
    background: url("../../../imgs/right_arrow.png") no-repeat;
    width: 0.34rem;
    height: 0.63rem;
    float: left;
    margin-top: 4.47rem;
    margin-left: 0.24rem;
    background-size: 100% 100%;
}

.right_arrow_active {
    background: url("../../../imgs/right_arrow_active.png") no-repeat;
    width: 0.34rem;
    height: 0.63rem;
    float: left;
    margin-top: 4.47rem;
    margin-left: 0.24rem;
    background-size: 100% 100%;
}